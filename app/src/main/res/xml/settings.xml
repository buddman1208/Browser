<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:key="main_settings"
        android:title="기본 설정">
        <ListPreference
            android:defaultValue="0"
            android:entries="@array/search_engine"
            android:entryValues="@array/search_engine_entries"
            android:key="search_engine"
            android:negativeButtonText="@null"
            android:positiveButtonText="@null"
            android:summary="Not Set"
            android:title="검색 엔진" />
        <EditTextPreference
            android:defaultValue="http://www.google.co.kr"
            android:inputType="textNoSuggestions"
            android:key="default_homepage_set"
            android:positiveButtonText="확인"
            android:title="홈페이지" />
    </PreferenceCategory>
    <!--<Preference android:title="@string/pref_title_system_sync_settings">-->
    <!--<intent android:action="android.settings.SYNC_SETTINGS" />-->
    <!--</Preference>-->
    <PreferenceCategory
        android:key="main_settings"
        android:title="정보">
        <PreferenceScreen
            android:key="developer_info"
            android:summary="개발팀 정보를 확인합니다."
            android:title="개발팀 정보">
        </PreferenceScreen>
        <PreferenceScreen
            android:key="pro_version"
            android:summary="프로 버전을 구입하여 더 많은 기능을 사용합니다"
            android:title="프로 버전 구입" />

    </PreferenceCategory>
</PreferenceScreen>
